{
  "env": {
    "node": true,
    "es6": true
  },
  "rules": {
    "complexity": [2, 5],
    "comma-spacing": 1,
    "consistent-return": 1,
    "eol-last": 1,
    "eqeqeq": [2, "smart"],
    "indent": [1, 2],
    "max-depth": [1, 3],
    "max-len": [2, 100],
    "max-nested-callbacks": [1, 3],
    "max-params": [1, 4],
    "max-statements": [1, 15],
    "no-multi-spaces": 1,
    "no-process-exit": 0,
    "no-trailing-spaces": 1,
    "no-underscore-dangle": 1,
    "no-unused-vars": 1,
    "no-undef": 1,
    "space-infix-ops": 1,
    "semi": [1, "always"],
    "semi-spacing": 1,
    "yoda": [1, "never"],
    "import/no-extraneous-dependencies": "off"
  },
  "plugins": [
  ],
  "parserOptions": {
    "sourceType": "module"
  },
  "globals": {
    "describe": true,
    "it": true,
    "before": true,
    "beforeEach": true,
    "after": true,
    "afterEach": true,
    "expect": true,
    "Async": true
  }
}
